<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE hibernate-mapping PUBLIC 
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class name="com.dto.StateDTO" table="Ste">

		<id name="sId">
		</id>
		<property name="sName" />
		<property name="sPopulation" />
		<many-to-one name="Country" class="com.dto.CountryDTO"
			cascade="all" />

	</class>

</hibernate-mapping>