<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE hibernate-mapping PUBLIC 
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class name="com.dto.DetailsDTO" table="Dtls">

		<id name="dId">
		</id>
		<property name="name" />
		<property name="livesOn" />
		<property name="numOfPopulation" />
		<many-to-one name="LivingOrganism"
			class="com.dto.LivingOrganismDTO" column="lId" cascade="all" />



	</class>

</hibernate-mapping>