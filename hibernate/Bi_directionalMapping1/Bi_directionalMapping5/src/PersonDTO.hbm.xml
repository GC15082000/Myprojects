<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC 
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class name="com.dto.PersonDTO" table="Person5">
		<id name="pID">
		</id>
		<property name="pName" />
		<property name="pCity" />		
		<many-to-one name="MovieTheatre" class="com.dto.MovieTheatreDTO"
	column="mNum" cascade="all" />
	</class>

</hibernate-mapping>