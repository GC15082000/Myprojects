<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE hibernate-mapping PUBLIC 
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class name="com.dto.SoldierDetailsDTO" table="SoldierDet">

		<id name="sId">
			<generator class="increment" />
		</id>
		<property name="sName" />
		<property name="typeOfField" />
		<many-to-one name="army" class="com.dto.Army" cascade="" />

	</class>

</hibernate-mapping>